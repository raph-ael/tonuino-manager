{
  "name": "tonuino-manager2",
  "productName": "tonuino-manager2",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "raphael",
    "email": "raphael.wintrich@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "tonuino_manager2"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                },
                {
                  "html": "./src/worker.html",
                  "js": "./src/worker.js",
                  "name": "worker_window"
                },
                {
                  "html": "./src/dialog.html",
                  "js": "./src/dialog.js",
                  "name": "dialog_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.51",
    "@electron-forge/maker-deb": "^6.0.0-beta.51",
    "@electron-forge/maker-rpm": "^6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.51",
    "@electron-forge/maker-zip": "^6.0.0-beta.51",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.51",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.51",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "electron": "18.3.7",
    "file-loader": "^6.0.0",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "diskusage": "^1.1.3",
    "drivelist": "^8.0.10",
    "electron-squirrel-startup": "^1.0.0",
    "file-type": "^14.3.0",
    "glob": "^7.1.6",
    "image-thumbnail": "^1.0.8",
    "jimp": "^0.10.3",
    "jquery": "^3.5.1",
    "jquery-ui-dist": "^1.12.1",
    "musicmetadata": "^2.0.5",
    "rimraf": "^3.0.2",
    "tmp": "^0.2.1"
  }
}
